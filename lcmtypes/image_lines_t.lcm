struct image_lines_t
{
    // time information
    int64_t transmissionTimeStamp;
    int64_t imageTimeStamp;

    // lines
    int16_t numLines;
    line_t line[numLines];

    // circles
    int16_t numCircles;
    circle_t circle[numCircles];

    //image info
    int32_t nSize;
    int8_t rawIplData[nSize];
    int32_t imageSize;
    int8_t imageData[imageSize];
}
